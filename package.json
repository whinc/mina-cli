{
  "name": "@whinc/mina-cli",
  "version": "0.1.3",
  "description": "",
  "main": "lib/gulpfile.js",
  "bin": {
    "mina": "bin/mina.js"
  },
  "scripts": {
    "logs": "pm2 logs --err --lines 30",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 ",
    "version": "npm run changelog && git commit -m \"chore(release): $npm_package_version\" CHANGELOG.md",
    "publish:npm": "npm login && npm publish --access=public",
    "semantic-release": "cross-env GH_TOKEN=$GH_TOKEN NPM_TOKEN=$NPM_TOKEN semantic-release"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "conventional-changelog-cli": "^2.0.12",
    "cross-env": "^5.2.0",
    "dayjs": "^1.8.9",
    "semantic-release": "^15.13.3",
    "semantic-release-cli": "^4.1.1"
  },
  "dependencies": {
    "vue-template-compiler": "^2.6.8",
    "@vue/component-compiler-utils": "^2.6.0",
    "del": "^4.0.0",
    "globby": "^9.1.0",
    "gulp": "^4.0.0",
    "gulp-debug": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-util": "^3.0.8",
    "less": "^3.9.0",
    "node-sass": "^4.11.0",
    "yargs": "^13.2.2"
  },
  "mina": {
    "src": "example/src",
    "dest": "example/dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whinc/mina-cli.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "ci": false,
    "plugins": ["@semantic-release/commit-analyzer", "@semantic-release/release-notes-generator", "@semantic-release/npm"]
  }
}
